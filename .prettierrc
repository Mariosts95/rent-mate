{
    "plugins": [
        "@trivago/prettier-plugin-sort-imports"
    ],
    "arrowParens": "always",
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "tabWidth": 2,
    "importOrder": [
        "^react",
        "^\\w",
        "/hooks/",
        "@mui/",
        "/store/",
        "/pages/",
        "/components/",
        "/assets/",
        "^./",
        "css$"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}